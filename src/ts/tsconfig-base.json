{
  "extends": "./tsconfig-core.json",
  "compilerOptions": {
    "outDir": "./build/tsc",
    "baseUrl": ".",
    "paths": {
      "basic/*": [
        "./basic/src/*"
      ],
      "app/domain": [
        "./app/domain/src/index"
      ],
      "app/editor": [
        "./app/editor/src/index"
      ],
      "app/framework": [
        "./app/framework/src/index"
      ],
      "app/root": [
        "./app/root/src/index"
      ],
      "app/storage": [
        "./app/storage/src/index"
      ],
      "lib/util": [
        "./lib/util/src/index"
      ]
    }
  },
  "outDir": "./build/tsc"
}
